    "server": "http-server ./dist"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^16.1.0",
    "@contentful/rich-text-types": "^16.2.0",
    "@srouse/-scottrouse-design-system": "v0.0.2",
    "scu-ssg": "0.1.18"
  },
  "devDependencies": {
    "contentful-readme-generator": "0.0.21",
    "http-server": "^14.1.1",
    "recursive-copy": "^2.0.14"
  }
}
